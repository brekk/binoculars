{
  "name": "binoculars",
  "version": "1.0.2",
  "description": "a tool for relating source files",
  "main": "index.js",
  "author": "brekk bockrath",
  "license": "ISC",
  "bin": {
    "binoculars": "./bin/cli.js"
  },
  "nyc": {
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "node_modules",
      "package-scripts.js",
      "*.spec.js",
      "index.js",
      "config",
      "coverage",
      "lib",
      "bin",
      "dist",
      "wallaby.js"
    ],
    "include": [
      "src/*.js"
    ],
    "all": true,
    "instrument": false,
    "sourceMap": false
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "files": [
      "src/*.spec.js",
      "src/*.fixture.*",
      "src/**/*.spec.js",
      "src/**/*.fixture.*"
    ],
    "source": [
      "src/*.js",
      "!src/*.spec.js",
      "src/**/*.js",
      "!src/**/*.spec.js"
    ],
    "concurrency": 10,
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "condition-circle": "^1.5.0",
    "cross-env": "^4.0.0",
    "dont-crack": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.13.3",
    "mkdirp": "^0.5.1",
    "nps": "^5.1.0",
    "nps-utils": "^1.2.0",
    "nyc": "^10.2.0",
    "precinct": "^3.6.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-post-replace": "^1.0.0",
    "rollup-plugin-progress": "^0.2.1",
    "rollup-plugin-shebang": "^0.1.4",
    "simple-commit-message": "^3.0.2",
    "webpack": "2"
  },
  "dependencies": {
    "builtin-modules": "^1.1.1",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "fluture": "^5.0.0",
    "get-es-imports-exports": "^1.0.8",
    "globby": "^6.1.0",
    "partial.lenses": "^10.1.1",
    "ramda": "^0.23.0",
    "to-absolute-glob": "^2.0.1"
  },
  "scripts": {
    "help!": "nps",
    "build": "nps build",
    "dist": "nps dist",
    "transpile": "nps transpile",
    "transpile.src": "nps transpile.src",
    "lint": "nps lint",
    "meta": "nps meta",
    "meta.auto": "nps meta.auto",
    "mkdir.coverage": "nps mkdir.coverage",
    "publish": "nps publish",
    "precommit": "nps precommit",
    "test": "nps test",
    "test.es6": "nps test.es6",
    "test.log": "nps test.log",
    "test.covered": "nps test.covered",
    "test.watch": "nps test.watch",
    "cost": "nps cost"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brekk/binoculars.git"
  },
  "keywords": [
    "source",
    "imports",
    "exports",
    "dependencies",
    "dependency"
  ],
  "bugs": {
    "url": "https://github.com/brekk/binoculars/issues"
  },
  "homepage": "https://github.com/brekk/binoculars#readme"
}
